<?xml version="1.0" ?>
<odoo>
    <data noupdate="1">
        <record id="cron_location_history" model="ir.cron">
            <field name="name">Get Location History</field>
            <field name="active" eval="True"/>
            <field name="model_id" ref="fleet_tracking.model_fleet_vehicle"/>
            <field name="state">code</field>
            <field name="code">model.get_location_history()</field>
            <field name="interval_number">1</field>
            <field name="interval_type">minutes</field>
            <field name="numbercall">-1</field>
        </record>
    </data>
</odoo>